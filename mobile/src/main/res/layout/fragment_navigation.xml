<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:app="http://schemas.android.com/apk/res-auto"
             xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             tools:context=".fragment.NavigationFragment"
             tools:showIn="@layout/activity_navigation">

    <com.skobbler.ngx.map.SKMapViewHolder
                  android:id="@+id/mapHolder"
                  android:layout_width="match_parent"
                  android:layout_height="match_parent" />

    <android.support.v7.widget.AppCompatImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/ic_place_white_36dp"
        android:backgroundTint="@color/colorPrimaryTint1"
        android:layout_marginBottom="18dp"
        android:layout_gravity="center"/>

</FrameLayout>
